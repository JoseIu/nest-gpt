{
  "version": 2,
  "env": {
    "OPENAI_AKI_KEY": "@openai_aki_key",
    "ASSISTANT_ID": "@assistant_id",
    "SERVER_URL": "@server_url",
    "PORT": "@port",
    "FRONT_URL": "@front_url",
    "FRONT_URL2": "@front_url2"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}
